{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "1",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "mode": "light",
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "200"
          },
          {
            "weight": "400"
          },
          {
            "weight": "700"
          },
          {
            "weight": "900"
          }
        ],
        "family": "Open Sans"
      }
    ]
  },
  "mainColors": {
    "base-100": "#ffffff",
    "secondary": "#EDE9E3",
    "tertiary": "#CDC6C3",
    "neutral": "#E6D1D88",
    "primary": "#56373C"
  }
}