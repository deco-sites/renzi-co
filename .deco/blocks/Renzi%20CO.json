{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "1",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
