{
  "name": "Product Page",
  "path": "/products/:slug",
  "sections": [
    {
      "__resolveType": "website/sections/Rendering/Lazy.tsx",
      "section": {
        "__resolveType": "Header Global"
      }
    },
    {
      "__resolveType": "website/sections/Rendering/Lazy.tsx",
      "section": {
        "__resolveType": "site/sections/Container.tsx",
        "sections": [
          {
            "section": {
              "__resolveType": "site/sections/Product/ProductDetails.tsx",
              "variant": "slider",
              "shipmentPolitics": {
                "label": " ",
                "link": " "
              },
              "shareableNetworks": [],
              "highlights": [],
              "page": {
                "__resolveType": "shopify/loaders/ProductDetailsPage.ts",
                "metafields": [],
                "slug": {
                  "__resolveType": "website/functions/requestToParam.ts",
                  "param": "slug"
                }
              }
            },
            "shadow": "none",
            "verticalSpacing": "both",
            "spacing": 0,
            "label": "ProductDetails",
            "withContainer": true
          }
        ]
      }
    },
    {
      "__resolveType": "website/sections/Rendering/Lazy.tsx",
      "section": {
        "__resolveType": "Produtos relacionados"
      }
    },
    {
      "__resolveType": "website/sections/Rendering/Lazy.tsx",
      "section": {
        "__resolveType": "Footer Global"
      }
    }
  ],
  "seo": {
    "__resolveType": "website/sections/Seo/SeoV2.tsx"
  },
  "__resolveType": "website/pages/Page.tsx"
}